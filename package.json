{
  "name": "business-public-page",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201",
    "start:development": "ng serve --configuration development --port 4201",
    "start:local": "ng serve --configuration local --port 4201",
    "start:production": "ng serve --configuration production --port 4201",
    "start:external": "ng serve --port 4201 --host 0.0.0.0 --disable-host-check",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "serve:ssr:business-public-page": "node dist/business-public-page/server/server.mjs",
    "lint": "ng lint",
    "build:firebase:development": "ng build --configuration development --output-path=./firebase/develop/dist",
    "build:firebase:production": "ng build --configuration production --output-path=./firebase/production/dist",
    "prepare": "ts-patch install && typia patch",
    "prettier": "npx prettier --write ."
  },
  "private": true,
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ],
    "moduleNameMapper": {
      "@src/(.*)": "<rootDir>/src/$1",
      "@environment/(.*)": "<rootDir>/src/environment/$1",
      "@assets/(.*)": "<rootDir>/src/assets/$1",
      "@scss/(.*)": "<rootDir>/src/assets/scss/$1",
      "@icon/(.*)": "<rootDir>/src/assets/icon/$1",
      "@app/(.*)": "<rootDir>/src/app/$1",
      "@module/(.*)": "<rootDir>/src/module/$1"
    }
  },
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/platform-server": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@angular/ssr": "^18.2.6",
    "@jest/globals": "^29.7.0",
    "@ngxs-labs/dispatch-decorator": "^5.0.0",
    "@ngxs-labs/select-snapshot": "^5.0.0",
    "@ngxs/devtools-plugin": "18.1.1",
    "@ngxs/logger-plugin": "18.1.1",
    "@ngxs/storage-plugin": "18.1.1",
    "@ngxs/store": "^18.1.1",
    "@p4ck493/checker": "^0.0.2",
    "bson-objectid": "^2.0.4",
    "express": "^4.18.2",
    "intl-tel-input": "^24.5.0",
    "luxon": "^3.5.0",
    "ngx-logger": "^5.0.12",
    "ngx-mask": "^18.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "typia": "^6.11.1",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.6",
    "@angular/cli": "^18.2.6",
    "@angular/compiler-cli": "^18.2.0",
    "@angular/localize": "^18.2.6",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@types/express": "^4.17.17",
    "@types/humanize-duration": "^3.27.4",
    "@types/intl-tel-input": "^18.1.4",
    "@types/luxon": "^3.4.2",
    "@types/node": "^18.18.0",
    "angular-eslint": "18.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.1",
    "jest-preset-angular": "^14.2.4",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.13",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.13",
    "typescript": "~5.5.2",
    "typescript-eslint": "8.2.0"
  }
}
