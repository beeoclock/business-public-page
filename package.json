{
  "name": "business-public-page",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201",
    "start:development:da": "ng serve --configuration da --port 4201",
    "start:development:uk": "ng serve --configuration uk --port 4201",
    "start:development:pl": "ng serve --configuration pl --port 4202",
    "start:development": "ng serve --configuration development --port 4201",
    "start:local": "ng serve --configuration local --port 4201",
    "start:production": "ng serve --configuration production --port 4201",
    "start:external": "ng serve --port 4201 --host 0.0.0.0 --disable-host-check",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "serve:ssr:business-public-page": "node dist/business-public-page/server/server.mjs",
    "lint": "ng lint",
    "prepare": "ts-patch install && typia patch",
    "prettier": "npx prettier --write .",
    "i18n:extract": "ng extract-i18n",
    "build:firebase:development": "ng build --configuration development --output-path=./firebase/develop/dist && npm run build:firebase:prepare_development_base --output-path=./firebase/develop/dist",
    "build:firebase:prepare_development_base": "ng build --configuration prepare_development_base --output-path=./firebase/develop/dist",
    "build:firebase:production": "ng build --configuration production --output-path=./firebase/production/dist && npm run build:firebase:prepare_production_base --output-path=./firebase/production/dist",
    "build:firebase:prepare_production_base": "ng build --configuration prepare_production_base --output-path=./firebase/production/dist"
  },
  "private": true,
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ],
    "moduleNameMapper": {
      "@src/(.*)": "<rootDir>/src/$1",
      "@environment/(.*)": "<rootDir>/src/environment/$1",
      "@assets/(.*)": "<rootDir>/src/assets/$1",
      "@scss/(.*)": "<rootDir>/src/assets/scss/$1",
      "@icon/(.*)": "<rootDir>/src/assets/icon/$1",
      "@app/(.*)": "<rootDir>/src/app/$1",
      "@module/(.*)": "<rootDir>/src/module/$1"
    }
  },
  "dependencies": {
    "@angular/common": "18.2.8",
    "@angular/compiler": "18.2.8",
    "@angular/core": "18.2.8",
    "@angular/forms": "18.2.8",
    "@angular/platform-browser": "18.2.8",
    "@angular/platform-browser-dynamic": "18.2.8",
    "@angular/platform-server": "18.2.8",
    "@angular/router": "18.2.8",
    "@angular/ssr": "18.2.8",
    "@jest/globals": "29.7.0",
    "@ng-icons/core": "29.5.1",
    "@ngxs-labs/dispatch-decorator": "5.0.0",
    "@ngxs-labs/select-snapshot": "5.0.0",
    "@ngxs/devtools-plugin": "18.1.1",
    "@ngxs/logger-plugin": "18.1.1",
    "@ngxs/storage-plugin": "18.1.1",
    "@ngxs/store": "18.1.1",
    "@p4ck493/checker": "0.0.2",
    "bson-objectid": "2.0.4",
    "express": "4.21.1",
    "intl-tel-input": "24.6.0",
    "luxon": "3.5.0",
    "ngx-logger": "5.0.12",
    "ngx-mask": "18.0.0",
    "rxjs": "7.8.1",
    "tslib": "2.7.0",
    "typia": "6.11.2",
    "zone.js": "0.14.10"
  },
  "devDependencies": {
    "@ng-icons/bootstrap-icons": "29.5.1",
    "@angular-devkit/build-angular": "18.2.8",
    "@angular/cli": "18.2.8",
    "@angular/compiler-cli": "18.2.8",
    "@angular/localize": "18.2.8",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.9",
    "@tailwindcss/typography": "0.5.15",
    "@types/express": "5.0.0",
    "@types/humanize-duration": "3.27.4",
    "@types/intl-tel-input": "18.1.4",
    "@types/jest": "29.5.13",
    "@types/luxon": "3.4.2",
    "@types/node": "22.7.5",
    "angular-eslint": "18.3.1",
    "autoprefixer": "10.4.20",
    "eslint": "9.12.0",
    "jest": "29.7.0",
    "jest-preset-angular": "14.2.4",
    "ng-extract-i18n-merge": "2.12.0",
    "postcss": "8.4.47",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.8",
    "tailwindcss": "3.4.13",
    "ts-patch": "3.2.1",
    "typescript": "5.5.4",
    "typescript-eslint": "8.8.1"
  }
}
